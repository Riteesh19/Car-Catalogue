    private void UploadActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_UploadActionPerformed
        JFileChooser chooser = new JFileChooser();
        chooser.showOpenDialog(null);
        File f = chooser.getSelectedFile();
        String filename = f.getAbsolutePath();

        Upload.setText(filename);
//        ImageIcon icon = new ImageIcon(filename);
//        Image image = icon.getImage().getScaledInstance(bioLabel.getWidth(),bioLabel.getHeight(),Image.SCALE_SMOOTH);

        ImageIcon imageIcon = new ImageIcon(new ImageIcon(filename).getImage().getScaledInstance(138, 152, Image.SCALE_DEFAULT));
        lblPhoto.setIcon(imageIcon);
    }//GEN-LAST:event_UploadActionPerformed

    

