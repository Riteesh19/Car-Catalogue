        Pattern pat = Pattern.compile(PATTERN);
        Matcher match = pat.matcher(txtOwnerName.getText());
         if (match.matches()) 
         {
             txtOwnername1.setText("valid");
         }
         else
         {
             txtOwnername1.setText("Invalid Entry");
         }
    }//GEN-LAST:event_txtOwnerNameActionPerformed

