    private void btnSaveActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnSaveActionPerformed
        // TODO add your handling code here:
        
        catalog.setLicensePlate(txtLicensePlate.getText());
        catalog.setCompany(txtCompany.getText());
        catalog.setColor(txtColor.getText());
        catalog.setModel(txtModel.getText());
        catalog.setYear(txtYear.getText());
        catalog.setEngineNumber(txtEngineNumber.getText());
        catalog.setSeatingCapacity(txtSeatingCapacity.getText());
        catalog.setLicensePlate(txtLicensePlate.getText());
        catalog.setOwnerName(txtOwnerName.getText());
        catalog.setOwnerTelephone(txtTelephone.getText());
        catalog.setOwnerEmail(txtEmail.getText());
        catalog.setOwnerLicense(txtLicense.getText());
        catalog.setOwnerSSN(txtSSN.getText());
        catalog.setOwnerAddress(txtAddress.getText());
        catalog.setServiceRecords(txtServiceRecords.getText());
        catalog.setWarrantyYear(txtWarrantyYear.getText());
        
        int count = 0;
        
//        String validTelephoneNumber = "(0/91)?[7-9][0-9]{9}";
//        Pattern pattern = Pattern.compile(validTelephoneNumber);
//        Matcher matcher = pattern.matcher(txtTelephone.getText());
//        if (!matcher.matches()) {
//            JOptionPane.showMessageDialog(this, "Enter correct Telephone Number");
//            count += 1;
//        }
//        if(count == 0){
//            catalog.setOwnerTelephone(txtTelephone.getText());
//        }
        String validEmail = "^(.+)@([^@]+[^.])$";
        Pattern pattern2 = Pattern.compile(validEmail);
        Matcher matcher2 = pattern2.matcher(txtEmail.getText());
        if (!matcher2.matches()) {
            JOptionPane.showMessageDialog(this, "Enter corect Email Address");
            count += 1;
        }
        if (count == 0) {
             catalog.setOwnerEmail(txtEmail.getText());
        }
        if(count == 0){
            JOptionPane.showMessageDialog(this, "Information Saved");
        }
        
    }//GEN-LAST:event_btnSaveActionPerformed

